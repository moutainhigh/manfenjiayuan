<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/material_white"
                android:baselineAligned="false"
                android:orientation="vertical">

    <include
        android:id="@+id/frame_top"
        layout="@layout/toolbar2"/>

    <LinearLayout
        android:id="@+id/ll_brief_1"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_below="@+id/frame_top"
        android:layout_marginEnd="@dimen/spacing_double"
        android:layout_marginStart="@dimen/spacing_double"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        android:padding="8dp">
        <!--门店-->
        <TextView
            android:id="@+id/tv_officename"
            style="@style/mfh_textview_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="门店："/>

        <!--收银员-->
        <TextView
            android:id="@+id/tv_humanName"
            style="@style/mfh_textview_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="帐号："/>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1">
            <!--交班时间-->
            <TextView
                android:id="@+id/tv_date"
                style="@style/mfh_textview_text"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="日结时间："/>
            <!--交班时间-->
            <com.bingshanguxue.vector_uikit.OptionalLabel
                android:id="@+id/label_date"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toEndOf="@+id/tv_date"
                app:mLabelView_inputHint=""
                app:mLabelView_inputTextColor="@color/black"
                app:mLabelView_inputTextColorHint="@color/black_35"
                app:mLabelView_inputTextSize="12sp"
                app:mLabelView_rightImageButtonEnabled="false"/>
        </RelativeLayout>

    </LinearLayout>

    <android.support.v4.widget.NestedScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/ll_brief_1"
        android:layout_marginBottom="@dimen/spacing_normal"
        android:layout_marginEnd="@dimen/spacing_4x"
        android:layout_marginStart="@dimen/spacing_4x"
        android:layout_marginTop="@dimen/spacing_normal"
        android:scrollbars="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/spacing_double"
            android:orientation="vertical">

            <!--头部-->
            <include
                layout="@layout/itemview_analysis_agg_header"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"/>

            <!--商品列表-->
            <!--android:layout_height="308dp"-->
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <!--空视图-->
                <TextView
                    android:id="@+id/empty_view"
                    style="@style/mfh_textview_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="暂无数据"
                    android:visibility="gone"/>

                <!--订单列表-->
                <com.mfh.framework.uikit.recyclerview.RecyclerViewEmptySupport
                    android:id="@+id/order_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

            </FrameLayout>

            <LinearLayout
                android:id="@+id/footerview_agg_datalist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/spacing_normal"
                android:visibility="gone">
                <!--营业额合计-->
                <TextView
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:gravity="center_vertical|end"
                    android:text="合计："/>

                <!--非现金收取-->
                <TextView
                    android:id="@+id/tv_agg_turnover"
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"/>

                <!--现金收取-->
                <TextView
                    android:id="@+id/tv_agg_salesBalance"
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"/>
            </LinearLayout>

            <include
                layout="@layout/itemview_analysis_acc_header"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:layout_marginTop="8dp"/>

            <!--280dp-->
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <!--空视图-->
                <TextView
                    android:id="@+id/paytype_empty_view"
                    style="@style/mfh_textview_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="暂无数据"
                    android:visibility="gone"/>

                <!--订单列表-->
                <com.mfh.framework.uikit.recyclerview.RecyclerViewEmptySupport
                    android:id="@+id/paytype_order_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

            </FrameLayout>

            <LinearLayout
                android:id="@+id/footerview_acc1_datalist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/spacing_normal"
                android:visibility="gone">
                <!--营业额合计-->
                <TextView
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:gravity="center_vertical|end"
                    android:text="合计："/>

                <!--非现金收取-->
                <TextView
                    android:id="@+id/tv_acc1_bcount"
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"/>

                <!--现金收取-->
                <TextView
                    android:id="@+id/tv_acc1_amount"
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"/>
            </LinearLayout>

            <include
                layout="@layout/itemview_analysis_acc_header"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:layout_marginTop="8dp"/>

            <!--280dp-->
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <!--空视图-->
                <TextView
                    android:id="@+id/acc_datalist2_empty_view"
                    style="@style/mfh_textview_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="暂无数据"
                    android:visibility="gone"/>

                <!--订单列表-->
                <com.mfh.framework.uikit.recyclerview.RecyclerViewEmptySupport
                    android:id="@+id/acc_datalist2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

            </FrameLayout>

            <LinearLayout
                android:id="@+id/footerview_acc2_datalist"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:padding="@dimen/spacing_normal"
                android:visibility="gone">
                <!--营业额合计-->
                <TextView
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2.5"
                    android:gravity="center_vertical|end"
                    android:text="合计："/>

                <!--非现金收取-->
                <TextView
                    android:id="@+id/tv_acc2_bcount"
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"/>

                <!--现金收取-->
                <TextView
                    android:id="@+id/tv_acc2_amount"
                    style="@style/mfh_textview_text"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"/>
            </LinearLayout>

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>

    <ImageButton
        android:id="@+id/fab_print"
        style="@style/FloatImageButton"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_margin="@dimen/spacing_double"
        android:contentDescription="@null"
        android:src="@mipmap/ic_print_white"/>

</RelativeLayout>
