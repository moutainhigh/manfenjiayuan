<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
                                            xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:background="@color/material_white"
                                            android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="@dimen/spacing_double"
        android:paddingBottom="@dimen/spacing_double">

        <!--商品-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_posgoods"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="top"
            app:src="@mipmap/ic_goods_warehouse"
            app:subText=""
            app:subTextSize="12sp"
            app:text="商品库"
            app:textSize="16sp"/>
        <!--&lt;!&ndash;米西生鲜&ndash;&gt;-->
        <!--<com.bingshanguxue.vector_uikit.SettingsItem-->
        <!--android:id="@+id/item_mixi_fresh"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="56dp"-->
        <!--app:src="@mipmap/ic_fresh"-->
        <!--app:position="center"-->
        <!--app:subText=""-->
        <!--app:text="米西生鲜"/>-->

        <!--设备-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_terminal"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="bottom"
            app:src="@mipmap/ic_terminal"
            app:subText=""
            app:subTextSize="12sp"
            app:text="设备编号"
            app:textSize="16sp"/>

        <!--系统键盘-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/toggleItem_softkeyboard"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="center"
            app:src="@mipmap/ic_keyboard"
            app:subTextSize="12sp"
            app:text="系统键盘"
            app:textSize="16sp"/>

        <!--内存-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_momory"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="center"
            app:src="@mipmap/ic_memory"
            app:subText="吃饱了别忘记减肥噢"
            app:subTextSize="12sp"
            app:text="内存"
            app:textSize="16sp"/>


        <!--语音播报-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/toggleItem_tts"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="center"
            app:src="@mipmap/ic_voice"
            app:subText="语音技术由科大讯飞提供"
            app:subTextSize="12sp"
            app:text="语音播报"
            app:textSize="16sp"/>

        <!--版本-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_version"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="bottom"
            app:src="@mipmap/ic_upgrade"
            app:subText=""
            app:subTextSize="12sp"
            app:text="应用版本"
            app:textSize="16sp"/>

        <!--一键反馈-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_onekey_feedback"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            app:position="bottom"
            app:src="@mipmap/ic_upgrade"
            app:subText=""
            app:subTextSize="12sp"
            app:text="一键反馈"
            app:textSize="16sp"/>

    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
