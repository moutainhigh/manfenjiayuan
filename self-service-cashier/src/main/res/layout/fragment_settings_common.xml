<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
                                            xmlns:app="http://schemas.android.com/apk/res-auto"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:background="@color/material_white"
                                            android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/spacing_double">

        <!--商品-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_posgoods"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="top"
            app:src="@mipmap/ic_goods_warehouse"
            app:subText=""
            app:subTextSize="12sp"
            app:text="商品库"
            app:textSize="16sp"/>
        <!--&lt;!&ndash;米西生鲜&ndash;&gt;-->
        <!--<com.bingshanguxue.vector_uikit.SettingsItem-->
        <!--android:id="@+id/item_mixi_fresh"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="48dp"-->
        <!--app:src="@mipmap/ic_fresh"-->
        <!--app:position="center"-->
        <!--app:subText=""-->
        <!--app:text="米西生鲜"/>-->

        <!--设备-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_terminal"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="bottom"
            app:src="@mipmap/ic_terminal"
            app:subText=""
            app:subTextSize="12sp"
            app:text="设备编号"
            app:textSize="16sp"/>

        <!--LED客显-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/toggleItem_leddisplay"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="@dimen/spacing_double"
            app:position="top"
            app:src="@mipmap/ic_rs232"
            app:subTextSize="12sp"
            app:text="LED客显"
            app:textSize="16sp"/>

        <!--打印机-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/toggleItem_printer"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_rs232"
            app:subTextSize="12sp"
            app:text="打印机(Gprinter)"
            app:textSize="16sp"/>

        <!--爱华电子秤-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/item_ahscale_rs232"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_rs232"
            app:subTextSize="12sp"
            app:text="爱华电子秤 (ACS-P215)"
            app:textSize="16sp"/>

        <!--寺冈电子秤-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/item_smscale_rs232"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_rs232"
            app:subTextSize="12sp"
            app:text="寺冈电子秤 (DS781)"
            app:textSize="16sp"/>
        <!--银联-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_umsips_rs232"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="bottom"
            app:src="@mipmap/ic_rs232"
            app:subText=""
            app:subTextSize="12sp"
            app:text="银联"
            app:textSize="16sp"/>

        <!--寺冈电子秤-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/item_smscale_ftp"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="@dimen/spacing_double"
            app:position="top"
            app:src="@mipmap/ic_ftp"
            app:subTextSize="12sp"
            app:text="FileZilla (寺冈电子秤)"
            app:textSize="16sp"/>

        <!--绿泰电子价签-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/item_greentags_webservice"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_tag"
            app:subTextSize="12sp"
            app:text="绿泰电子价签"
            app:textSize="16sp"/>

        <!--系统键盘-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/toggleItem_softkeyboard"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_keyboard"
            app:subTextSize="12sp"
            app:text="系统键盘"
            app:textSize="16sp"/>


        <!--内存-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_momory"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_memory"
            app:subText="吃饱了别忘记减肥噢"
            app:subTextSize="12sp"
            app:text="内存"
            app:textSize="16sp"/>


        <!--语音播报-->
        <com.bingshanguxue.vector_uikit.ToggleSettingItem
            android:id="@+id/toggleItem_tts"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="center"
            app:src="@mipmap/ic_voice"
            app:subText="语音技术由科大讯飞提供"
            app:subTextSize="12sp"
            app:text="语音播报"
            app:textSize="16sp"/>

        <!--版本-->
        <com.bingshanguxue.vector_uikit.SettingsItem
            android:id="@+id/item_version"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            app:position="bottom"
            app:src="@mipmap/ic_upgrade"
            app:subText=""
            app:subTextSize="12sp"
            app:text="应用版本"
            app:textSize="16sp"/>

    </LinearLayout>

</android.support.v4.widget.NestedScrollView>
