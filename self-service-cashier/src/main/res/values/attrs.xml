<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color"/>
        <attr name="pstsUnderlineColor" format="color"/>
        <attr name="pstsDividerColor" format="color"/>
        <attr name="pstsIndicatorHeight" format="dimension"/>
        <attr name="pstsUnderlineHeight" format="dimension"/>
        <attr name="pstsDividerPadding" format="dimension"/>
        <attr name="pstsTabPaddingLeftRight" format="dimension"/>
        <attr name="pstsScrollOffset" format="dimension"/>
        <attr name="pstsTabBackground" format="reference"/>
        <attr name="pstsShouldExpand" format="boolean"/>
        <attr name="pstsTextAllCaps" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="InputNumberLabelView">
        <attr name="inputNumberLabelView_title" format="string"/>
        <attr name="inputNumberLabelView_titleWidth" format="dimension"/>

        <attr name="inputNumberLabelView_inputTextColor" format="color"/>
        <attr name="inputNumberLabelView_inputTextSize" format="dimension"/>
        <attr name="inputNumberLabelView_inputTextColorHint" format="color"/>
        <attr name="inputNumberLabelView_inputHint" format="string"/>

        <attr name="inputNumberLabelView_rightButtonWidth" format="dimension"/>
        <attr name="inputNumberLabelView_rightImageButtonSrc" format="reference"/>
        <attr name="clearOnClickDel" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="MOrderLabelView">
        <attr name="mOrderLabelView_text" format="string"/>
        <attr name="mOrderLabelView_textColor" format="color"/>
        <attr name="mOrderLabelView_textSize" format="dimension"/>
    </declare-styleable>

</resources>