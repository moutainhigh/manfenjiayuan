    android:orientation="horizontal">

    <TextView
        style="@style/mfh_form_header_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="批发商" />


    <TextView
        style="@style/mfh_form_header_text"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="箱规" />

    <TextView
        style="@style/mfh_form_header_text"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="起订量" />

    <TextView
        style="@style/mfh_form_header_text"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="采购价" />


</LinearLayout>
